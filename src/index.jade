doctype html
html(lang="en-US")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name='viewport', content='initial-scale=1.0, user-scalable=no')
    block title
      title Fun Experiments with Google Maps
    link(rel="stylesheet", href="css/main.css")
  body
    .container
      #searchControls
        form
          input(type="text" id="placesSearch" placeholder="Your Destination")
          input(id="searchButton" type="button"  value="Find It!")
    #map-canvas
    
    script(src='//code.jquery.com/jquery-1.11.0.min.js')
    script(src='//ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/jquery-ui.min.js')
    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAI7DrVYSz0-Z-zE3iV85PmRy9f-6MU4Uc')
    script(language='javascript', type='text/javascript', src='js/map.js')
    script(language='javascript', type='text/javascript', src='js/search.js')
    script(src="js/main.js")
